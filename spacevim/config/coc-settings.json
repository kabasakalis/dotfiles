{
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": [
        "c",
        "cpp",
        "cuda",
        "objc",
        "objcpp, cmake, txt"
      ],
      "trace.server": "verbose",
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cacheDirectory": "/tmp/ccls"
      }
    }
  },
  "coc.preferences.autoTrigger": "none",
  "coc.preferences.triggerAfterInsertEnter": false,
  "coc.preferences.timeout": 2000,
  "coc.preferences.noselect": true,
  "coc.preferences.formatOnType": false,
  "coc.preferences.minTriggerInputLength": 2,
  "coc.preferences.snippetIndicator": "â¤",
  "coc.preferences.jumpCommand": "edit",
  "coc.preferences.watchmanPath": "",
  "coc.preferences.diagnostic.enable": true,
  "coc.preferences.diagnostic.signOffset": 1000,
  "coc.preferences.diagnostic.errorSign": "âœ–",
  "coc.preferences.diagnostic.warningSign": "â¤",
  "coc.preferences.diagnostic.infoSign": "ğŸ›ˆ",
  "coc.preferences.diagnostic.hintSign": ">",
  "java.format.enabled":true,
  "java.format.comments.enabled":true,
  "java.trace.server": "verbose"
}
